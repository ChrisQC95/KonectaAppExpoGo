{
  "expo": {
    "name": "Konecta",
    "slug": "assiskey",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/konecta.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bms.assiskey",
      "buildNumber": "1.0.4",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Konecta to access your camera.",
        "NSLocationWhenInUseUsageDescription": "Allow Konecta to use your location."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyClYwasQvblGLZZKZcVLPjuGoRgdIKVV1Q"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/konecta.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.bms.assiskey",
      "versionCode": 13
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-barcode-scanner"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0931fedd-be9d-4e5f-bb50-25593f60db1f"
      }
    },
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true
    }
  }
}
